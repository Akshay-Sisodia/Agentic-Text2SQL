{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://agentic-text2sql-api.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "cd frontend && pnpm install && pnpm run build:no-check",
  "outputDirectory": "frontend/dist",
  "framework": null
} 